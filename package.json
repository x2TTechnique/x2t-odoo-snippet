        "keybindings": [],
        "languages": [
            {
                "id": "javascript",
                "aliases": [
                    "JavaScript",
                    "js"
                ],
                "extensions": [
                    ".js"
                ],
                "filenames": [
                    "jsconfig.json"
                ],
                "filenamePatterns": [
                    "*.js"
                ],
                "mimetypes": [
                    "application/javascript",
                    "text/javascript"
                ]
            },
            {
                "id": "typescript",
                "aliases": [
                    "TypeScript",
                    "ts"
                ],
                "extensions": [
                    ".ts",
                    ".tsx"
                ],
                "filenames": [
                    "tsconfig.json"
                ],
                "filenamePatterns": [
                    "*.ts",
                    "*.tsx"
                ],
                "mimetypes": [
                    "application/typescript",
                    "text/typescript"
                ]
            },
            {
                "id": "python",
                "aliases": [
                    "Python",
                    "py"
                ],
                "extensions": [
                    ".py"
                ],
                "filenames": [
                    "requirements.txt",
                    "setup.py"
                ],
                "filenamePatterns": [
                    "*.py"
                ],
                "mimetypes": [
                    "text/x-python"
                ]
            },
            {
                "id": "xml",
                "aliases": [
                    "XML"
                ],
                "extensions": [
                    ".xml"
                ],
                "filenames": [
                    "settings.xml"
                ],
                "filenamePatterns": [
                    "*.xml"
                ],
                "mimetypes": [
                    "application/xml",
                    "text/xml"
                ]
            },
            {
                "id": "csv",
                "aliases": [
                    "CSV"
                ],
                "extensions": [
                    ".csv"
                ],
                "filenames": [
                    "data.csv"
                ],
                "filenamePatterns": [
                    "*.csv"
                ],
                "mimetypes": [
                    "text/csv"
                ]
            }
        ],
        "themes": [
            {
                "label": "Odoo Dark",
                "id": "odoo.dark",
                "uiTheme": "vs-dark",
                "path": "./themes/odoo.dark.json"
